<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="my.orange.testnote.controller.MainSceneController">
   <TableView fx:id="table" AnchorPane.bottomAnchor="45.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
              AnchorPane.topAnchor="0.0">
      <columns>
         <TableColumn fx:id="dateColumn" prefWidth="100.0" text="Date"/>
         <TableColumn fx:id="textColumn" prefWidth="498.0" text="Note"/>
      </columns>
   </TableView>
   <Button mnemonicParsing="false" onAction="#pressed" text="Add" AnchorPane.bottomAnchor="10.0"
           AnchorPane.rightAnchor="10.0"/>
</AnchorPane>
